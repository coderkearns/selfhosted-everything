<%- include("../includes/header", {
    title: "Notes",
    styles: ["/assets/maintext.css", "/assets/notes.css"]
}) %>

<div class="content">

  <h1>Notes</h1>

  <p class="link"><a href="/notes/new">New Note</a></p>

  <h2>Your Notes</h2>

  <ul>
    <% for (const note in notes) { %>
    <li><a href="/notes/<%= note %>"><%= notes[note].title %></a></li>
    <% } %>
  </ul>

</div>

